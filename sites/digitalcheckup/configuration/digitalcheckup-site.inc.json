{
  "domains": [
    "digitalcheckup.local",
    "digitalcheckup.org"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../../../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/production/digitalcheckup/",
    "title": "Digital Checkup",
    "menus": "{:./digitalcheckup-menus.json}",
    "checkup": "{:./checkup.json}",
    "areas": "{:./areas.json}",
    "elasticsearchUrl": "{$../../elasticsearch-url.txt}",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    }
  }
}