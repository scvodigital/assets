{
  "domains": [
    "localhost",
    "digitalparticipation.local",
    "127.0.0.1",
    "test.digitalparticipation.scot",
    "digitalparticipation.scot",
    "www.digitalparticipation.scot"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../../../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/digitalparticipation/",
    "title": "Scotland's Digital Participation Charter",
    "menus": "{:./digitalparticipation-menus.json}",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    }
  }
}