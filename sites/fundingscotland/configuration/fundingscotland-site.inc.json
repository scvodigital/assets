  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../../../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/fundingscotland/",
    "title": "Funding Scotland",
    "menus": "{:./fundingscotland-menus.json}",
    "elasticsearchUrl": "{$../elasticsearch-url.txt}",
    "emailCampaignName": "fs-saved-searches",
    "shortlistCampaignName": "fs-saved-funds",
    "terms": "{:./terms.json}",
    "domain-map": "{:./domain-map.json}",
    "site-configurations": "{:./site-configurations.json}",
    "globalNoteStatuses": [
      "Application in process",
      "Application sent and received / under review",
      "Application rejected",
      "Application successful",
      "Open to reapply",
      "Not open to apply"
    ],
    "termTypeLabels": {
      "geographical_areas_funded": {
        "singular": "Region",
        "plural": "Regions"
      },
      "region_group": {
        "singular": "Region",
        "plural": "Regions"
      },
      "type_of_cost": {
        "singular": "Type of cost",
        "plural": "Type of cost"
      },
      "type_of_funding": {
        "singular": "Type of funding",
        "plural": "Type of funding"
      },
      "fund_status": {
        "singular": "Status",
        "plural": "Status"
      },
      "charities_only": {
        "singular": "Charities only",
        "plural": "Charities only"
      },
      "activities": {
        "singular": "Activity",
        "plural": "Activities"
      },
      "beneficiaries": {
        "singular": "Beneficiary",
        "plural": "Beneficiaries"
      }
    },
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    },
    "commonTasks": "{:./common-tasks.json}"
  }
}