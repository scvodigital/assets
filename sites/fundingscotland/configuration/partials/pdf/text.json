{
  {{>pdf_styles this}}
  {{#if link}}
    {{>pdf_key_value key="link" value=link}},
  {{/if}}
  {{#if linkToPage}}
    {{>pdf_key_value key="linkToPage" value=linkToPage}},
  {{/if}}
  {{#if @partial-block}}
    "text": [
      {{> @partial-block}}
    ]
  {{else}}
    {{>pdf_key_value key="text" value=(default text (default default ' '))}}
  {{/if}}
}