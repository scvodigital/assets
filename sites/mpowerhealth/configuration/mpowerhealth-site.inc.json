{
  "domains": [
    "mpowerhealth.eu",
    "test.mpowerhealth.eu",
    "mpowerhealth.local",
    "dumfriesgalloway.mpowerhealth.eu",
    "mpowerhealth-dumfriesgalloway.local"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../../../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/mpowerhealth/",
    "title": "mPower Health",
    "menus": "{:./mpowerhealth-menus.json}",
    "elasticsearchUrl": "{$../../elasticsearch-url.txt}",
    "domain-map": "{:./domain-map.json}",
    "site-configurations": "{:./site-configurations.json}",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    },
    "commonTasks": "{:./common-tasks.json}"
  }
}