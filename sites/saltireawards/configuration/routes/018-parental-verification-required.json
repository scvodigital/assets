{
  "name": "parental-verification-required",
  "pattern": "/parental-verification-required",
  "debug": true,
  "tasks": [
    "context.metaData.commonTasks.authRequired",
    {
      "name": "apexUrl",
      "taskModule": "render",
      "renderer": "handlebars",
      "config": {
        "output": "data",
        "template": "/SaltireUser/?user_id={{@root.data.auth.uid}}"
      }
    },
    {
      "name": "accountInfo",
      "taskModule": "salesforceApex",
      "renderer": "handlebars",
      "config": {
        "connection": "scvoAlexSandbox",
        "output": "data",
        "method": "GET",
        "apexClassPath": ">data.apexUrl"
      }
    },
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.default",
            "parts": {
              "head": ">context.metaData.handlebars.templates.pages_head-tags",
              "body": ">context.metaData.handlebars.templates.pages_parental-verification-required",
              "foot": ""
            },
            "contentType": "text/html"
          }
        }
      }
    }

  ]
}