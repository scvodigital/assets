{
  "name": "test-page",
  "pattern": "/test-page",
  "debug": true,
  "tasks": [
    "context.metaData.commonTasks.authNotRequired",
    "context.metaData.commonTasks.getAccountInfo",

    "context.metaData.commonTasks.adminRequired",
    {
      "name": "claimHoursResult",
      "taskModule": "render",
      "renderer": "handlebars",
      "config": {
        "output": "data",
        "template": ">context.metaData.handlebars.templates.tasks_hours-claim-body"
      }
    },
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.default",
            "parts": {
              "head": ">context.metaData.handlebars.templates.pages_head-tags",
              "body": ">context.metaData.handlebars.templates.pages_test",
              "sidebar": ">context.metaData.handlebars.templates.account_sidebar",
              "foot": ">context.metaData.handlebars.templates.pages_foot"
            },
            "contentType": "text/html"
          }
        }
      }
    }
  ]
}