{{#if @root.data.primaryResponse.hits.hits.[0] ~}}
<title>SCVO – Policies on {{#each @root.data.primaryResponse.hits.hits.[0]._source.policy_topics_slugs}}{{#compare this "===" @root.request.params.topic}}{{{lookup @root.data.primaryResponse.hits.hits.[0]._source.policy_topics @index}}}{{/compare}}{{/each}}</title>
<meta property="og:title" content="SCVO – Policies on {{#each @root.data.primaryResponse.hits.hits.[0]._source.policy_topics_slugs}}{{#compare this "===" @root.request.params.topic}}{{{lookup @root.data.primaryResponse.hits.hits.[0]._source.policy_topics @index}}}{{/compare}}{{/each}}" data-dynamic="true">
<link rel="alternate" type="application/rss+xml" title="SCVO – Policies on {{#each @root.data.primaryResponse.hits.hits.[0]._source.policy_topics_slugs}}{{#compare this "===" @root.request.params.topic}}{{{lookup @root.data.primaryResponse.hits.hits.[0]._source.policy_topics @index}}}{{/compare}}{{/each}}" href="https://scvo.org{{@root.request.params.uri.pathname}}?_view=rss" />
{{#with @root.data.primaryResponse.hits.hits.[0]._source ~}}
<meta property="og:image" content="https://storage.googleapis.com/scvo-cms/scvo-cms-content/images/logos/scvo-logo-square.png" data-dynamic="true">
<meta property="og:image:width" content="96" data-dynamic="true">
<meta property="og:image:height" content="96" data-dynamic="true">
{{#if rendered.json_ld ~}}
<script type="application/ld+json" data-dynamic="true">
{{{rendered.json_ld}}}
</script>
{{/if ~}}
{{/with ~}}
{{else}}
<title>SCVO – Scottish Council for Voluntary Organisations</title>
<meta property="og:title" content="SCVO – Scottish Council for Voluntary Organisations" data-dynamic="true">
{{/if ~}}