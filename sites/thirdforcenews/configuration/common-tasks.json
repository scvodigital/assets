{
  "authNotRequired": {
    "name": "auth",
    "taskModule": "firebaseAuth",
    "config": {
      "tokenPath": "request.cookies.tfn_token",
      "cookieName": "tfn_cookie",
      "cookieOptions": {
        "maxAge": 1209600000,
        "secure": true,
        "path": "/"
      },
      "appName": "thirdforcenews",
      "noTokenRoute": "",
      "notAuthenticatedRoute": ""
    }
  },
  "authRequired": {
    "name": "auth",
    "taskModule": "firebaseAuth",
    "config": {
      "tokenPath": "request.cookies.tfn_token",
      "cookieName": "tfn_cookie",
      "cookieOptions": {
        "maxAge": 1209600000,
        "secure": true,
        "path": "/"
      },
      "appName": "thirdforcenews",
      "noTokenRoute": "sign_in",
      "notAuthenticatedRoute": "sign_in"
    }
  },
  "menus": {
    "name": "menus",
    "taskModule": "elasticsearch",
    "config": {
      "connectionStringTemplate": "{$../../elasticsearch-url.txt}",
      "elasticsearchConfig": {
        "apiVersion": "5.6"
      },
      "queryTemplates": {
        "index": "{$../../elasticsearch-index.txt}",
        "type": "tfn-page",
        "template": ">context.metaData.handlebars.queries.pages_menus"
      }
    },
    "renderer": "handlebars"
  }
}